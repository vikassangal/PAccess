      <xsd:enumeration value="CODE_28_VALUE_SISTER-IN-LAW"/>
      <xsd:enumeration value="CODE_29_VALUE_MOTHER-IN-LAW"/>
      <xsd:enumeration value="CODE_30_VALUE_FATHER-IN-LAW"/>
      <xsd:enumeration value="CODE_31_VALUE_GUARDIAN"/>
      <xsd:enumeration value="CODE_32_VALUE_CONSERVATOR"/>
