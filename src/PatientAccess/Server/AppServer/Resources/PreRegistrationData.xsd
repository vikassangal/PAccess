<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema targetNamespace="http://www.dell.com/services/healthcare/patientaccess/pre-registration/v1.0"
            xmlns="http://www.dell.com/services/healthcare/patientaccess/pre-registration/v1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            elementFormDefault="qualified">

  <xsd:import namespace="http://microsoft.com/wsdl/types/" schemaLocation="GuidType.xsd" >
    <xsd:annotation>
      <xsd:documentation>Importing this schema to help the .NET framework map the Id element to the built in .Net Guid type. This schema was generated by the XSD.exe tool when run on a class with a Guid type property.</xsd:documentation>
    </xsd:annotation>
  </xsd:import>

  <xsd:element name="preRegistration">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="1" maxOccurs="1" name="id" xmlns:q1="http://microsoft.com/wsdl/types/" type="q1:guid">
          <xsd:annotation>
            <xsd:documentation>
              This will be a unique id in the form of a GUID that will uniquely identify the message.
              It is based on a type in the imported schema rather than a simple string type to help
              the .NET schema to class generator (XSD.exe) create a Guid type property for the Id element
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>

        <xsd:element name="dateTimeOfSubmission" type="xsd:dateTime" minOccurs="1" maxOccurs="1"/>

        <xsd:element name="facility" type="facilityType" minOccurs="1" maxOccurs="1"/>

        <xsd:element name="patient" type="patientType" minOccurs="1" maxOccurs="1"/>

        <xsd:element name="visit" type="visitType" minOccurs="1" maxOccurs="1"/>

      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="facilityType">
    <xsd:sequence>
      <xsd:element name="code" type="facilityCodeType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="serviceLocation" minOccurs="0" maxOccurs="1" type="serviceLocationType"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="facilityCodeType">
    <xsd:restriction base="xsd:string">
      <xsd:length value="3"/>
      <xsd:pattern value="[A-Z]{3,3}"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="serviceLocationType">
    <xsd:sequence>
      <xsd:element name="name" type="serviceLocationNameType" minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="serviceLocationNameType">
    <xsd:restriction base="startWithLetterRestLettersNumbersSpaceHyphenPeriodApostrophe">
      <xsd:minLength value="5"/>
      <xsd:maxLength value="40"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="visitType">
    <xsd:annotation>
      <xsd:documentation>admission information</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="returningPatient" type="xsd:boolean" minOccurs="0" maxOccurs="1" />
      <xsd:element name="previousVisitName" type="nameWithComma" minOccurs="0" maxOccurs="1" />
      <xsd:element name="admittingPhysicianName" type="physicianNameType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="treatingPhysicianName" type="physicianNameType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="primaryCarePhysician" type="physicianNameType" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Primary Care Physician / Family Doctor</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="expectedAdmissionDateTime" type="xsd:dateTime" minOccurs="1" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>format needs to be YYYY-MM-DD</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="procedureTreatmentOrTest" type="procedureType" minOccurs="1" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Type of Procedure/Treatment/Test</xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element name="insuranceInformation" minOccurs="0" maxOccurs="1">
        <xsd:complexType>
          <xsd:annotation>
            <xsd:documentation>Company telephone number goes under the address element</xsd:documentation>
          </xsd:annotation>
          <xsd:sequence>
            <xsd:element name="primaryInsurance" type="insuranceType" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="secondaryInsurance" type="insuranceType" minOccurs="0" maxOccurs="1"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

      <xsd:element name="bestWayToContact" type="wayToContactType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="bestTimeToContact" type="timeToContactType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="preferredMethodOfPayment" type="methodOfPaymentType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="newsletterRegistration" type="newsletterRegistrationType" minOccurs="1" maxOccurs="1"/>

    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="patientType">
    <xsd:sequence>

      <xsd:element name="name" type="nameType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="placeOfBirth" type="placeOfBirthType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="dateOfBirth" type="xsd:date" minOccurs="1" maxOccurs="1" />

      <xsd:element name="socialSecurityNumber" type="socialSecurityNumberType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="gender" type="genderType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="maritalStatus" type="maritalStatusType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="race" type="raceType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="ethnicity" type="ethnicityType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="preferredLanguage" type="preferredLanguageType" minOccurs="0" maxOccurs="1"/>

      <xsd:element name="religiousPreference" type="religionType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="address" type="addressType" minOccurs="1" maxOccurs="1"/>

      <xsd:element name="phoneNumber" type="telephoneNumberType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="cellPhoneNumber" type="telephoneNumberType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="emailAddress" type="emailAddressType" minOccurs="0" maxOccurs="1" />

      <xsd:element name="employment" type="employmentType" minOccurs="1" maxOccurs="1" />

      <xsd:element name="emergencyContact1" type="emergencyContactType" minOccurs="1" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Emergency Notification</xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element name="emergencyContact2" type="emergencyContactType" minOccurs="1" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Nearest Relative or Friend (not living with you)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element name="guarantor" type="guarantorType" minOccurs="1" maxOccurs="1" />

    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="maritalStatusType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_D_VALUE_DIVORCED"/>
      <xsd:enumeration value="CODE_X_VALUE_LG SEPARAT"/>
      <xsd:enumeration value="CODE_L_VALUE_LIFE PARTN"/>
      <xsd:enumeration value="CODE_M_VALUE_MARRIED"/>
      <xsd:enumeration value="CODE_S_VALUE_SINGLE"/>
      <xsd:enumeration value="CODE_U_VALUE_UNKNOWN"/>
      <xsd:enumeration value="CODE_F_VALUE_WIDOWED"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="raceType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_5_VALUE_ASIAN"/>
      <xsd:enumeration value="CODE_2_VALUE_BLACK"/>
      <xsd:enumeration value="CODE_4_VALUE_NATIV AMER"/>
      <xsd:enumeration value="CODE_6_VALUE_OTHER"/>
      <xsd:enumeration value="CODE_3_VALUE_PACIFC ISL"/>
      <xsd:enumeration value="CODE_8_VALUE_TEST SYNC"/>
      <xsd:enumeration value="CODE_7_VALUE_UNKNOWN"/>
      <xsd:enumeration value="CODE_1_VALUE_WHITE"/>
    </xsd:restriction>

  </xsd:simpleType>

  <xsd:simpleType name="genderType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_M_VALUE_MALE"/>
      <xsd:enumeration value="CODE_F_VALUE_FEMALE"/>
      <xsd:enumeration value="CODE_U_VALUE_UNKNOWN"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="nameType">
    <xsd:sequence>
      <xsd:element name="first" maxOccurs="1" minOccurs="1" type="firstNameType" />
      <xsd:element name="last" type="lastNameType" maxOccurs="1" minOccurs="1" />
      <xsd:element name="maiden" minOccurs="0" maxOccurs="1">
        <xsd:simpleType>
          <xsd:restriction base="startWithLetterRestLettersNumbersSpaceHyphen">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="25"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="middleInitial" type="middleInitialType" minOccurs="0" maxOccurs="1"/>

    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="firstNameType">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space or hyphen</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="startWithLetterRestLettersNumbersSpaceHyphen">
      <xsd:maxLength value="15" />
      <xsd:minLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="middleInitialType">
    <xsd:restriction base="xsd:string">
      <xsd:length value="1"/>
      <xsd:pattern value="[A-Z]"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="lastNameType">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space or hyphen</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="startWithLetterRestLettersNumbersSpaceHyphen">
      <xsd:maxLength value="25" />
      <xsd:minLength value="1"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="fullNameType">
    <xsd:restriction base="startWithLetterRestLettersNumbersSpaceHyphen">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="23"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="nameWithComma">
    <xsd:restriction base="startWithLetterRestLettersSpaceHyphenComma">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="25"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="physicianNameType">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space or hyphen</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="startWithLetterRestLettersNumbersSpaceHyphen">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="44" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="placeOfBirthType">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z][ A-Z\-.]*"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="socialSecurityNumberType">
    <xsd:annotation>
      <xsd:documentation>format needs to be 123-123-1234</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="\d{3}\-\d{2}-\d{4}"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="lettersNumberSpaceAndHyphen">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space or hyphen</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9 \-]*" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="startWithLetterRestLettersNumbersSpaceHyphen">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space or hyphen</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z][ A-Z0-9\-]*" />
    </xsd:restriction>
  </xsd:simpleType>
  
  <xsd:simpleType name="startWithLetterRestLettersNumbersSpaceHyphenPeriodApostrophe">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space or hyphen</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z][ A-Z0-9\-.']*" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="startWithLetterRestLettersSpaceHyphenComma">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z][ A-Z\-,]*" />
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="startWithLetterOrNumberRestLettersNumbersSpaceHyphenPeriod">
    <xsd:annotation>
      <xsd:documentation>First character has to be a letter rest can be letters numbers space, hyphen or period</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z0-9][ A-Z0-9\-.]*"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="startWithLetterOrNumberRestLettersNumbersSpaceHyphen">
    <xsd:annotation>
      <xsd:documentation>
        First character has to be a letter rest can be letters numbers space, hyphen or period
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="" />
      <xsd:pattern value="[A-Z0-9][ A-Z0-9-]*"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="ethnicityType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_1_VALUE_HISPANIC"/>
      <xsd:enumeration value="CODE_2_VALUE_NON-HISPANIC"/>
      <xsd:enumeration value="CODE_3_VALUE_UNKNOWN"/>
    </xsd:restriction>
  </xsd:simpleType>

