  <xsd:complexType name="insuranceType">
    <xsd:sequence>
      <xsd:element name="companyName" minOccurs="1" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Currently patient access is not directly populating this field therefore it is not being restricted to upper case letters for now</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1"/>
            <xsd:maxLength value="40"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="preCertificationTelephoneNumber" type="telephoneNumberType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="subscriberName" type="nameType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="subscriberSocialSecurityNumber" type="socialSecurityNumberType" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="subscriberDateOfBirth" type="xsd:date" minOccurs="0" maxOccurs="1"/>
      <xsd:element name="policyNumber" minOccurs="1" maxOccurs="1">
        <xsd:simpleType>
            <xsd:restriction base="lettersNumberSpaceAndHyphen">
                <xsd:minLength value="1" />
                <xsd:maxLength value="20" />
            </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="policyGroupName" minOccurs="0" maxOccurs="1">
        <xsd:simpleType>
          <xsd:restriction base="lettersNumberSpaceAndHyphen">
            <xsd:minLength value="1" />
            <xsd:maxLength value="40" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="address" type="addressType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="phoneNumber" type="telephoneNumberType" minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="guarantorType">
    <xsd:sequence>
      <xsd:element name="name" minOccurs="1" maxOccurs="1" type="nameType" />
      <xsd:element name="relationship" type="relationshipType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="socialSecurityNumber" type="socialSecurityNumberType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="address" type="addressType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="phoneNumber" type="telephoneNumberType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="businessPhoneNumber" type="telephoneNumberType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="employment" type="employmentType" minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="preferredLanguageType">
    <xsd:annotation>
      <xsd:documentation>In case of California facilities other language will be specified by using the otherLanguage type. This is done to enforce the rule that when other language is specified for California then the name of that language should also be given</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="predefinedLanguge" type="languageType"/>
      <xsd:element name="otherLanguage" type="otherLanguageType"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:simpleType name="otherLanguageType">
    <xsd:restriction base="startWithLetterRestLettersSpaceHyphenComma">
      <xsd:minLength value="1"/>
      <xsd:maxLength value="20"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="employmentType">
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="fullTimePartTimeMilitaryOrOther" type="fullTimePartTimeMilitaryOrOtherEmploymentType"/>
        <xsd:element name="retiredNotEmployedOrSelfEmployed" type="retiredNotEmployedOrSelfEmployedStatusType"/>
      </xsd:choice>
      <xsd:element name="telephoneNumber" type="telephoneNumberType" minOccurs="0" maxOccurs="1" />
      <xsd:element name="industry" type="industryType" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>occupation/industry</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="fullTimePartTimeMilitaryOrOtherEmploymentType">
    <xsd:sequence>
      <xsd:element name="status" type="fullTimePartTimeMilitaryOrOtherEmploymentStatusType" />
      <xsd:element name="name" type="fullNameType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="address" type="addressType" minOccurs="1" maxOccurs="1" />
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="retiredNotEmployedOrSelfEmployedStatusType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_3_VALUE_NOT-EMPLOYED"/>
      <xsd:enumeration value="CODE_5_VALUE_RETIRED"/>
      <xsd:enumeration value="CODE_4_VALUE_SELF-EMPLOYED"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="fullTimePartTimeMilitaryOrOtherEmploymentStatusType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_1_VALUE_EMPLOYED FULL TIME"/>
      <xsd:enumeration value="CODE_2_VALUE_EMPLOYED PART-TIME"/>
      <xsd:enumeration value="CODE_6_VALUE_ON ACTIVE MILITARY DUTY"/>
      <xsd:enumeration value="CODE_9_VALUE_OTHER"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="wayToContactType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Phone"/>
      <xsd:enumeration value="Email"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="methodOfPaymentType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Credit Card"/>
      <xsd:enumeration value="Check"/>
      <xsd:enumeration value="Cash"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="timeToContactType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Morning"/>
      <xsd:enumeration value="Afternoon"/>
      <xsd:enumeration value="Evening"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:complexType name="newsletterRegistrationType">
    <xsd:sequence>
      <xsd:element name="receiveNewsletter" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="emailAddress" type="emailAddressType" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

</xsd:schema>
