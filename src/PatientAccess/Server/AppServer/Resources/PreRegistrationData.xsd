  <xsd:complexType name="addressType">
    <xsd:choice maxOccurs="1" minOccurs="1">
      <xsd:element name="usAddress" type="usAddressType"/>
      <xsd:element name="canadianAddress" type="canadianAddressType"/>
      <xsd:element name="mexicanAddress" type="mexicanAddressType"/>
      <xsd:element name="nonUSNonCanadaAddress" type="nonUSNonCanadaAddressType"/>
    </xsd:choice>
  </xsd:complexType>

  <xsd:complexType name="usAddressType">
    <xsd:sequence>
      <xsd:element name="addressLine" type="addressLineType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="city" minOccurs="1" maxOccurs="1" type="cityType"/>
      <xsd:element name="stateOrTerritory" type="usStateOrTerritoryType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="zipCode" type="usAndMexicanZipCodeType" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="canadianAddressType">
    <xsd:sequence>
      <xsd:element name="addressLine" type="addressLineType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="city" minOccurs="1" maxOccurs="1" type="cityType"/>
      <xsd:element name="province" type="canadianProvinceType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="zipCode" type="canadianZipCodeType" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="mexicanAddressType">
    <xsd:sequence>
      <xsd:element name="addressLine" type="addressLineType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="city" minOccurs="1" maxOccurs="1" type="cityType"/>
      <xsd:element name="state" type="mexicanStateType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="zipCode" type="usAndMexicanZipCodeType" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="nonUSNonCanadaAddressType">
    <xsd:sequence>
      <xsd:element name="country" type="nonUSNonCanadaCountryType" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="addressLine" type="addressLineType" minOccurs="1" maxOccurs="1" />
      <xsd:element name="city" minOccurs="1" maxOccurs="1" type="cityType" />
      <xsd:element name="zipCode" type="internationalZipCodeType" minOccurs="0" maxOccurs="1" />
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="nonUSNonCanadaCountryType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CODE_MEX_VALUE_Mexico"/>
      <xsd:enumeration value="CODE_ABW_VALUE_Aruba"/>
      <xsd:enumeration value="CODE_AFG_VALUE_Afghanistan"/>
      <xsd:enumeration value="CODE_AGO_VALUE_Angola"/>
